{
  "version": 2,
  "name": "intern-whatsapp-assistant",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.js"
    }
  ]
}
